            <maxY value="8.0" type="double" dim="kpc"/>
            <minZ value="-5.0" type="double" dim="kpc"/>
            <maxZ value="5.0" type="double" dim="kpc"/>
            <step value="0.1" type="double" dim="kpc"/>
            <starsRegionRadius value="1.0" type="double" dim="kpc"/>
        </ConfigProcessing>
    </Kinematic>
</GalaxyKinematic>
