<?xml version="1.0" encoding="UTF-8"?>
<GalaxyKinematic>
    <Catalog>
        <ConfigTable>
            <fileType value="0" type="int"/> <!-- 0 = auto, 1 = TSV, 2 = CSV -->
            <isWithHeader value="true" type="bool"/>
            <coordInRad value="false" type="bool"/>
            <!-- Column numbers in the star catalog -->
            <cn_ra value="1" type="int"/>
            <cn_de value="3" type="int"/>
            <cn_plx value="5" type="int"/>
            <cn_muRa value="7" type="int"/>
            <cn_muDe value="9" type="int"/>
            <cn_Vr value="11" type="int"/>
        </ConfigTable>
        <Pixelization>
            <minX value="-10.0" type="double" dim="kpc"/>
            <maxX value="10.0" type="double" dim="kpc"/>
            <minY value="-10.0" type="double" dim="kpc"/>
            <maxY value="10.0" type="double" dim="kpc"/>
            <minZ value="-5.0" type="double" dim="kpc"/>
            <maxZ value="5.0" type="double" dim="kpc"/>
